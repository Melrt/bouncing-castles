<div class="container">
  <div class="row">
    <div class="col-sm form-basics">
      <h2 class="mt-3 text-center">My new bouncing castle</h2>
      <%= simple_form_for [:renter, @new_castle] do |f| %>
        <div class="form-row">
          <div class="col-sm-12 col-md-6">
            <%= f.input :name %>
          </div>
          <div class="col-sm-12 col-md-6">
            <%= f.input :photo %>
            <%= f.input :photo_cache, as: :hidden %>
          </div>
        </div>
        <div class="form-row">
          <div class="col-sm-12 col-md-4">
           <%= f.input :nb_players, collection: 1..100 %>
          </div>
          <div class="col-sm-12 col-md-4">
           <%= f.input :day_price %>
          </div>
          <div class="col-sm-12 col-md-4">
           <%= f.input :address %>
          </div>
        </div>
        <div class="form-row">
          <div class="col-sm-12">
            <%= f.input :description %>
          </div>
        </div>

        <%= f.submit "Add a Bouncing Castle!!!", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>


